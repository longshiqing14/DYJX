span.xtt-splitor {
  color: #aaa;
}
.bottom-action-bar {
  position: fixed;
  left: 0;
  bottom: 0;
  right: 0;
  background: #f7f7f7;
  padding: 4px;
  -webkit-box-shadow: 0 1px 64px #ccc;
  -moz-box-shadow: 0 1px 64px #ccc;
  box-shadow: 0 1px 64px #ccc;
}
.bottom-action-bar button.mui-btn-block {
  margin-top: 0;
  margin-bottom: 0;
}
.mui-input-row .float-btn {
  position: absolute;
  top: 50%;
  -webkit-transform: translateY(-50%);
  transform: translateY(-50%);
  right: 4px;
  text-align: center;
  width: auto;
  padding: 8px 8px;
}
.xtt-statusLabel {
  padding-top: 6px;
  font-size: 12px;
  line-height: 14px;
  color: gray;
  text-align: center;
}
.mui-loader {
  position: absolute;
  top: 25%;
  width: 100%;
  height: 60%;
  color: #888;
  font-size: 16px;
  line-height: 30px;
  text-align: center;
}
.xtt-prompt-bk {
  position: absolute;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  z-index: 8888;
  background-color: rgba(0, 0, 0, 0.6);
}
.xtt-prompt {
  position: absolute;
  width: 80%;
  top: 15%;
  left: 10%;
  z-index: 8889;
}
.xtt-prompt .xtt-prompt-wrapper {
  background-color: #eeeeee;
  padding: 12px;
  -webkit-border-radius: 6px;
  -moz-border-radius: 6px;
  border-radius: 6px;
}
.xtt-prompt .xtt-prompt-wrapper .title {
  padding-bottom: 8px;
}
.xtt-prompt .xtt-prompt-wrapper .content {
  padding-bottom: 8px;
}
.xtt-prompt .xtt-prompt-wrapper .content textarea {
  padding: 1px;
  margin: 0;
}
.xtt-prompt .xtt-prompt-wrapper .action {
  text-align: center;
}
.xtt-prompt .xtt-prompt-wrapper .action button {
  width: 45%;
  margin-bottom: 0;
}
.xtt-pay-prompt {
  position: absolute;
  left: 10px;
  top: 10px;
  right: 10px;
  bottom: 10px;
  z-index: 8890;
  background-color: #eeeeee;
  padding: 8px;
  -webkit-border-radius: 6px;
  -moz-border-radius: 6px;
  border-radius: 6px;
  color: #333333;
}
.xtt-pay-prompt .title {
  padding-bottom: 8px;
  padding-left: 8px;
}
.xtt-pay-prompt .content {
  position: absolute;
  top: 33px;
  left: 1px;
  right: 1px;
  bottom: 76px;
  overflow: auto;
}
.xtt-pay-prompt .sum {
  position: absolute;
  left: 8px;
  right: 8px;
  bottom: 41px;
  padding: 6px 8px;
  text-align: right;
}
.xtt-pay-prompt .action {
  position: absolute;
  left: 8px;
  right: 8px;
  bottom: 6px;
  text-align: center;
}
.xtt-pay-prompt .action button {
  width: 30%;
  margin-bottom: 0;
}
.xtt-update-prompt {
  position: absolute;
  left: 10px;
  top: 10px;
  right: 10px;
  bottom: 30px;
  z-index: 8891;
  background-color: #eeeeee;
  padding: 8px;
  -webkit-border-radius: 6px;
  -moz-border-radius: 6px;
  border-radius: 6px;
  color: #333333;
}
.xtt-update-prompt .title {
  padding-bottom: 8px;
  padding-left: 8px;
}
.xtt-update-prompt .content {
  position: absolute;
  top: 33px;
  left: 1px;
  right: 1px;
  bottom: 106px;
  padding: 6px 16px;
  font-size: 14px;
  line-height: 23px;
  color: #555555;
  overflow: auto;
}
.xtt-update-prompt .content #update-description {
  margin-top: 4px;
  color: #999;
}
.xtt-update-prompt .sum {
  position: absolute;
  left: 8px;
  right: 8px;
  bottom: 41px;
  padding: 6px 8px;
  text-align: right;
}
.xtt-update-prompt .progress {
  position: absolute;
  left: 8px;
  right: 8px;
  bottom: 41px;
  padding: 6px 8px;
}
.xtt-update-prompt .progress .progress-box {
  height: 25px;
  border: solid 1px #8f8f94;
}
.xtt-update-prompt .progress .progress-box .progress-line {
  height: 23px;
  background-color: #2ac845;
  width: 0%;
}
.xtt-update-prompt .progress .log-text {
  height: 23px;
  font-size: 14px;
  line-height: 23px;
  color: #999999;
  text-overflow: ellipsis;
  overflow: hidden;
  white-space: nowrap;
}
.xtt-update-prompt .action {
  position: absolute;
  left: 8px;
  right: 8px;
  bottom: 6px;
  text-align: center;
}
.xtt-update-prompt .action button {
  width: 30%;
  margin-bottom: 0;
}
