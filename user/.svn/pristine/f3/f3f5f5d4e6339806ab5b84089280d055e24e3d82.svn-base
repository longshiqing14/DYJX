#import "GJTableCellBorderView.h"

@implementation GJTableCellBorderView
-(id)initWithBorderColor:(UIColor *)color
{
    if (self = [super init]) {
        [self setBorderColor:color];
    }
    return self;
}

-(void)setBorderColor:(UIColor *)borderColor
{
    [super setBackgroundColor:borderColor];
}

-(UIColor *)borderColor
{
    return [super backgroundColor];
}

